<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mapping.Uczniowie" table="Uczniowie" schema="edziennik">
        <id name="uczenId">
            <column name="uczen_id" sql-type="int(11)"/>
        </id>
        <property name="imie">
            <column name="imie" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="nazwisko">
            <column name="nazwisko" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="klasaId">
            <column name="klasa_id" sql-type="int(11)"/>
        </property>
        <property name="rodzicId">
            <column name="rodzic_id" sql-type="int(11)"/>
        </property>
        <property name="kontoId">
            <column name="konto_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>