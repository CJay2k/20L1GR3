<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mapping.KlasyPrzedmioty" table="Klasy_Przedmioty" schema="edziennik">
        <id name="klasyPrzedmiotyId">
            <column name="klasy_przedmioty_id" sql-type="int(11)"/>
        </id>
        <property name="klasaId">
            <column name="klasa_id" sql-type="int(11)"/>
        </property>
        <property name="przedmiotId">
            <column name="przedmiot_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>